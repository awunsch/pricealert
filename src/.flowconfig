[ignore]
.*/build/.*
.*/node_modules/.*/node_modules/.*
.*/node_modules/babel-core/.*
.*/node_modules/babel-loader/.*
.*/node_modules/babel-register/.*
.*/node_modules/config-chain/.*
.*/node_modules/flow-bin/.*
.*/node_modules/inline-style-prefixer/.*
.*/node_modules/invariant/.*
.*/node_modules/mkdirp/.*
.*/node_modules/nodemon/.*
.*/node_modules/npmconf/.*
.*/node_modules/react-hot-loader/.*
.*/node_modules/rimraf/.*
.*/node_modules/watchify/.*
.*/node_modules/webpack-dev-server/.*
.*/node_modules/webpack/.*
.*/node_modules/react-event-listener/.*
.*/node_modules/draft-js/.*
.*/node_modules/radium/.*

[include]

[libs]
flow-typed

[options]
module.system=haste
module.name_mapper='.*\(.css\)' -> 'CSSModule'
module.name_mapper='.*\(.less\)' -> 'CSSModule'
module.name_mapper='^actions$' -> '<PROJECT_ROOT>/js/actions'
module.name_mapper='^reducer$' -> '<PROJECT_ROOT>/js/reducer'
module.name_mapper='^middleware$' -> '<PROJECT_ROOT>/js/middleware'
module.name_mapper='^types$' -> '<PROJECT_ROOT>/js/types'
module.name_mapper='^components\/\(.*\)$' -> '<PROJECT_ROOT>/js/components/\1'
module.name_mapper='^utils\/\(.*\)$' -> '<PROJECT_ROOT>/js/utils/\1'
esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable
